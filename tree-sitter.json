{
  "grammars": [
    {
      "name": "zsh",
      "camelcase": "Zsh",
      "scope": "source.zsh",
      "path": ".",
      "file-types": [
        "zsh",
        ".zshrc",
        ".zprofile"
      ],
      "injection-regex": "^zsh$",
      "first-line-regex": "^#!.*\\b(zsh)\\b.*$"
    }
  ],
  "metadata": {
    "version": "0.23.3",
    "license": "MIT",
    "description": "Bash grammar for tree-sitter",
    "authors": [
      {
        "name": "Max Brunsfeld",
        "email": "maxbrunsfeld@gmail.com"
      },
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-bash"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
